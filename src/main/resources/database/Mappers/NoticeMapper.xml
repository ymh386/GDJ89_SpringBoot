<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.moon.app.board.notice.NoticeDAO">
  	<select id="getList" resultType="com.moon.app.board.notice.NoticeVO">
	  	SELECT * FROM NOTICE_TBL
	  		
  	</select>
  	<select id="getDetail" resultType="com.moon.app.board.notice.NoticeVO" parameterType="com.moon.app.board.notice.NoticeVO">
  		SELECT * FROM NOTICE_TBL
  		WHERE BOARD_NUM=#{boardNum}
  	</select>
  	
  	<insert id="add" parameterType="com.moon.app.board.notice.NoticeVO">
  		INSERT INTO NOTICE_TBL VALUES (null, #{boardTitle}, #{boardContents}, now(), 0, #{userName})
  	</insert>
  </mapper>